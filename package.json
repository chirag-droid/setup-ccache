{
  "name": "ccache-setup",
  "version": "0.0.1",
  "private": true,
  "description": "Github action to speedup cmake builds using ccache",
  "main": "lib/restore.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build lib/restore.js -o dist/restore && ncc build lib/cache.js -o dist/cache",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chirag-droid/setup-ccache"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "cmake",
    "cpp",
    "ccache",
    "cache"
  ],
  "author": "Chirag Singla <chirag.singla.pi@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/chirag-droid/setup-ccache",
  "dependencies": {
    "@actions/cache": "^1.0.7",
    "@actions/core": "^1.5.0",
    "@actions/exec": "^1.1.0",
    "@actions/github": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.1.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.4.1",
    "typescript": "^4.4.3"
  }
}
